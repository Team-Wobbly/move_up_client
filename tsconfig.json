{
  "compilerOptions": {
    "target": "es6",
    "module":"commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "types": [],
    "outDir": "build/app"
  },
  "exclude": [
      "node_modules"
  ]
}
